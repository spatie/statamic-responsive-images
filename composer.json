{
    "name": "spatie/statamic-responsive-images",
    "license": "MIT",
    "type": "statamic-addon",
    "description": "Responsive Images for Statamic",
    "autoload": {
        "psr-4": {
            "Spatie\\ResponsiveImages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponsiveImages\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Responsive Images",
            "description": "Responsive Images for Statamic"
        },
        "laravel": {
            "providers": [
                "Spatie\\ResponsiveImages\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "spatie/pest-plugin-snapshots": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    }
}
