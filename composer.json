{
    "name": "spatie/statamic-responsive-images",
    "license": "MIT",
    "type": "statamic-addon",
    "description": "Responsive Images for Statamic 3",
    "autoload": {
        "psr-4": {
            "Spatie\\ResponsiveImages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponsiveImages\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Responsive Images",
            "description": "Responsive Images for Statamic 3"
        },
        "laravel": {
            "providers": [
                "Spatie\\ResponsiveImages\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "3.3.*"
    },
    "minimum-stability": "beta",
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "spatie/laravel-ray": "^1.10"
    }
}
